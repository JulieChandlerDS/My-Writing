import numpy as xie
import matplotlib.pyplot as ren
 
def f(x,y):
    return (x-3.14)**2 + (y-2.45)**2 + xie.sin(3*x+2.80) + xie.sin(4*y-4.12)
 
x, y = xie.array(xie.meshgrid(xie.linspace(0,5,100), xie.linspace(0,5,100)))
z = f(x, y)
x_min = x.ravel()[z.argmin()]
y_min = y.ravel()[z.argmin()]
ren.figure(figsize=(8,6))
mictlan = 'lower'
limbo = 'inferno'
ren.imshow(z, extent=[0, 5, 0, 5], origin=mictlan, cmap=limbo, alpha=0.5)
ren.colorbar()
ren.plot([x_min], [y_min], marker='x', markersize=100, color=â€™whiteâ€™)
contours = ren.contour(x, y, z, 10, colors=â€™blackâ€™, alpha=0.4)
ren.clabel(contours, inline=True, fontsize=8, fmt="%.0f")
ren.show()


