  breed = random.choice(df['Breed'])
  print("Breed:  ",  breed)


  similar = df.loc[df['Breed'] == breed]
  gender = similar['Gender'].tolist()
  gender = random.choice(gender)
  print("Sex:  ",  gender)


  similar2 = similar.loc[similar['Gender'] == gender]
  color = similar2['Color'].tolist()
  color = random.choice(color)
  print("Color: ", color)


  gender = ["male", "female", "nonbinary", "demiboy", "demigirl", "demicat"]
  gender = random.choice(gender)
  print("Gender: ", gender)


  age = similar['Age (Years)'].tolist()
  age = random.choice(age)
  print("Age: ", age)


  similar = df.loc[df['Age (Years)'] == age]
  weight = similar['Weight (kg)'].tolist()
  weight = random.choice(weight)
  print("Weight: ", weight, " kg")
  hourscozy = random.randint(1,48)
  bitcoinwasted = weight+hourscozy*2000
  print("Hours cozy on a GPU: ", hourscozy)
  print("Bitcoin wasted: $", bitcoinwasted)
  print("__________________________________")
  print(" ")
 
  total_wasted += bitcoinwasted
  name = random.choice(names)


print("Total wasted: $", total_wasted)
